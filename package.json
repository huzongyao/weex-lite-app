    "babel-preset-stage-0": "^6.24.1",
    "bui-weex": "0.4.19",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-plugin-for-multihtml": "^2.30.2",
    "ip": "^1.1.5",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.9.3",
    "portfinder": "^1.0.13",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
