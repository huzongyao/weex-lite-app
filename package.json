    "vue-template-compiler": "^2.5.11",
    "webpack": "^3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-merge": "^4.1.0",
    "webpack-uglify-parallel": "^0.1.4",
    "weex-loader": "^0.7.2",
    "weex-ui": "0.6.16",
    "weex-vue-precompiler": "^0.1.17"
  }
